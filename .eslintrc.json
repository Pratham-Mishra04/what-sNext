
{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "warn",
        "no-console" : "warn",
        "linebreak-style":"off",
        "import/extensions":"off",
        "no-underscore-dangle":"warn",
        "no-param-reassign":"warn",
        "no-unused-vars":"warn",
        "consistent-return":"off"
    }
}